{
  "id": "blog-post",
  "label": "Blog post",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "Uid",
          "placeholder": "URL slug for the article"
        }
      },
      "featuredImage": {
        "type": "Image",
        "config": {
          "label": "Featured image",
          "constraint": {
            "width": 2000,
            "height": 1500
          },
          "thumbnails": []
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": "Title of the article"
        }
      },
      "publishDate": {
        "type": "Date",
        "config": {
          "label": "Publish date",
          "placeholder": "Date the article was published"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "subtitle": {
              "type": "SharedSlice"
            },
            "image": {
              "type": "SharedSlice"
            },
            "quote": {
              "type": "SharedSlice"
            },
            "text": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}